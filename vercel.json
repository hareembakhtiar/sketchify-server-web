{
  "version": 2,
  "rewrites": [
    { "source": "/api/socket", "destination": "/api/socket.js" },
    { "source": "/socket.io/(.*)", "destination": "/api/socket.js" }
  ],
  "headers": [
    {
      "source": "/api/socket",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://sketchify-client-web.vercel.app" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
